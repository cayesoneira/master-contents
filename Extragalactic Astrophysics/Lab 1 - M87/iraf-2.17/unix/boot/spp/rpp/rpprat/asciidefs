define (ETB,23)
define (CAN,24)
define (EM,25)
define (SUB,26)
define (ESC,27)
define (FS,28)
define (GS,29)
define (RS,30)
define (US,31)
define (SP,32)
define (BLANK,SP)
define (BANG,33)
define (DQUOTE,34)
