define (SQUOTE,39)
define (LPAREN,40)
define (RPAREN,41)
define (ASTERISK,42)
define (CLOSURE,ASTERISK)
define (STAR,ASTERISK)
define (PLUS,43)
define (COMMA,44)
define (MINUS,45)
define (DASH,MINUS)
define (PERIOD,46)
define (SLASH,47)
define (DIG0,48)
define (DIG1,49)
define (DIG2,50)
define (DIG3,51)
define (DIG4,52)
define (DIG5,53)
define (DIG6,54)
define (DIG7,55)
define (DIG8,56)
define (DIG9,57)
define (COLON,58)
define (SEMICOL,59)
define (LESS,60)
define (EQUALS,61)
define (GREATER,62)
define (QMARK,63)
define (ANY,QMARK)
define (ESCAPE,64)

define (BIGA,65)
define (BIGB,66)
define (BIGC,67)
define (BIGD,68)
